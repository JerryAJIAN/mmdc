<div class="page-header">
  <%= link_to wanteds_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_wanted_path(@wanted), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show wanted</h1>
</div>

<dl class="dl-horizontal">
  <dt>File name:</dt>
  <dd><%= @wanted.file_name %></dd>

  <dt>Folder name:</dt>
  <dd><%= @wanted.folder_name %></dd>

  <dt>Extension:</dt>
  <dd><%= @wanted.extension %></dd>

  <dt>Lock version:</dt>
  <dd><%= @wanted.lock_version %></dd>

</dl>
